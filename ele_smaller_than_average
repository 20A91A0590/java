import java.util.Scanner;
class ele
{
    public static void main(String args[])
{
    Scanner s=new Scanner(System.in);
    int n=s.nextInt();
    int a[]=new int[50];
    int i,cnt=0;
    for(i=0;i<n;i++)
    {
        a[i]=s.nextInt();
    }
    double avg=0;
    for(i=0;i<n;i++)
    {
        avg=avg+a[i];
    }
    avg=avg/n;
    for(i=0;i<n;i++)
    {
    if(a[i]<=avg)
    cnt++;
    }
    System.out.println(cnt);
}
}
