import java.util.Scanner;
class reverse
{
    public static void main(String args[])
    {
        Scanner r=new Scanner(System.in);
        int n=r.nextInt();
        int d,sum=0,temp;
        temp=n;
        while(n>0)
        {
            d=n%10;
            sum=sum*10+d;
            n=n/10;
        }
        System.out.println(sum);
    }
}
